<!DOCTYPE html>
<html>
  <%- include('../../layout/head') %>
  <body>
    <%- include('../../layout/header') %>
    <div id='myLinks'>
      <div id='pokeball'>
        <a class='upperBall'></a>
        <a class='middleBall'></a>
        <a class='pageLink' href='/favorites'></a>
        <a class='lowerBall'></a>
      </div>
    </div>
    <div class="showContainer">
      <ul>
        <% pokemon.forEach(mans => { %>
          <li class="pokemonChoice">
            <%- include('../pokemon/show.ejs', { pokemon : mans }) %>
          </li>
        <% }); %>
      </ul>
    </div>
    <%- include('../../layout/footer') %>
    <%- include('../../layout/navScript') %>
  </body>
</html>